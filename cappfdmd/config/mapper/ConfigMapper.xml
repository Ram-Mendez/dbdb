<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.1//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iberdrola.cappfdmd.config.mapper.ConfigMapper">



    <select id="getValByCode" parameterType="map" resultType="String">
        SELECT value
        FROM CONFIG
        WHERE code = #{code}
    </select>


</mapper>